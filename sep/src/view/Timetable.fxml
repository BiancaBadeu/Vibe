<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="700.0" maxWidth="1300.0" minHeight="700.0" minWidth="1300.0" prefHeight="700.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.TimetableController">
   <children>
      <Label alignment="CENTER" minWidth="600.0" prefHeight="55.0" prefWidth="798.0" text="Timetable">
         <font>
            <Font name="System Bold" size="38.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_RIGHT" prefHeight="32.0" prefWidth="600.0" spacing="10">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select view" />
            <ChoiceBox fx:id="classFilter" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox prefHeight="512.0" prefWidth="800.0">
         <children>
            <VBox alignment="TOP_CENTER" spacing="190">
               <children>
                  <DatePicker />
                  <Button mnemonicParsing="false" onAction="#backPressed" prefHeight="56.0" prefWidth="118.0" text="Go back">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox prefHeight="512.0" prefWidth="1043.0">
               <children>
                  <TableView fx:id="timetable" prefHeight="510.0" prefWidth="820.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
