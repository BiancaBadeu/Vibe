public class Teacher
{
  private String id;

  public Teacher(String id)
  {
    this.id = id;
  }

  public String getId()
  {
    return id;
  }

  public String toString()
  {
      return "ID: " + this.id;
  }

  public boolean equals(Object obj)
  {
    if(!(obj instanceof Teacher))
      return false;
    Teacher other = (Teacher) obj;
    return id.equals(other.id);
  }
}
